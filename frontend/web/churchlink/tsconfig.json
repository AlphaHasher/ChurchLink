{
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "types": [
      "cypress"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@sections/*": [
        "src/features/admin/components/WebBuilder/sections/*"
      ]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.d.ts",
    "cypress/support/commands.js"
  ]
}