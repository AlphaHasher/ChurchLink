    },
    {
      "book": "Hosea",
      "type": "one_to_one",
      "from": {"tx": "rst", "chapter": 13, "verse": 16},
      "to":   {"tx": "kjv", "chapter": 14, "verse": 1}
    },

    {
      "book": "Joel",
      "type": "range_shift",
      "note": "KJV 2:28-32 == RST 3:1-5",
      "from": {"tx": "kjv", "chapter": 2, "start": 28, "end": 32},
      "to":   {"tx": "rst", "chapter": 3, "start": 1,  "end": 5}
    },
    {
      "book": "Joel",
      "type": "chapter_remap",
      "note": "KJV ch.3 maps to RST ch.4 (same verses)",
