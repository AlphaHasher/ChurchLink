{
  "pairs": [
    {
      "book": "Jonah",
      "type": "one_to_one",
      "from": {"tx": "kjv", "chapter": 1, "verse": 17},
      "to":   {"tx": "rst", "chapter": 2, "verse": 1}
    },
    {
      "book": "Jonah",
