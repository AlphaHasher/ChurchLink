        {"from": {"tx": "rst", "chapter": 4}, "to": {"tx": "kjv", "chapter": 3}}
      ]
    },

    {
      "book": "Malachi",
      "type": "range_shift",
      "note": "KJV 4:1-6 == RST 3:19-24",
      "from": {"tx": "kjv", "chapter": 4, "start": 1, "end": 6},
      "to":   {"tx": "rst", "chapter": 3, "start": 19, "end": 24}
