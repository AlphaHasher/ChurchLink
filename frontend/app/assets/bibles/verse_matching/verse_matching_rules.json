      "from": {"tx": "kjv", "chapter": 4, "start": 1, "end": 3},
      "to":   {"tx": "rst", "chapter": 3, "start": 31, "end": 33}
    },
    {
      "book": "Daniel",
      "type": "range_shift",
      "from": {"tx": "rst", "chapter": 3, "start": 31, "end": 33},
      "to":   {"tx": "kjv", "chapter": 4, "start": 1, "end": 3}
    },

    {
      "book": "Hosea",
      "type": "one_to_one",
      "from": {"tx": "kjv", "chapter": 14, "verse": 1},
